{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client", "preact"],
    "lib": ["WebWorker"],
    "isolatedModules": true,
    "moduleResolution": "node",
    "target": "es6",
    "module": "commonjs",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true
  },
  "exclude": ["./node_modules/**/*"]
}
